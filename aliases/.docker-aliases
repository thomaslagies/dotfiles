docwipe () {
	echo "Stopping all containers"
	docker stop `docker ps -qa` $1 2>&1
	echo "Removing all containers"
	docker rm `docker ps -qa` $1 2>&1
	echo "Removing all volumes"
	docker volume rm $(docker volume ls -qf) $1 2>&1
	echo "Removing all networks"
	docker network rm `docker network ls -q` $1 2>&1
}

alias dco=docker-compose
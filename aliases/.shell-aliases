alias ..='cd ..'
alias ...='cd ../../'
alias ....='cd ../../../'
alias cl='clear'
alias cg="cd $HOME/git; ls"
alias ll='ls -la'
alias ls='ls --color=auto'
alias grep='grep --color=auto'
alias egrep='egrep --color=auto'
alias fgrep='fgrep --color=auto'
alias r='exec zsh'

function pkey () {
  cat ~/.ssh/github_id_rsa.pub | xclip -selection clipboard
}